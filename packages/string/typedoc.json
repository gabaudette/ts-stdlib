{
	"$schema": "https://typedoc.org/schema.json",
	"entryPointStrategy": "expand",
	"plugin": ["typedoc-plugin-markdown"],
	"theme": "markdown",
	"entryPoints": [
		"src/case-conversion",
		"src/template-interpolation",
		"src/trims",
		"src/word-wrap"
	],
	"tsconfig": "./tsconfig.json",
	"out": "docs",
	"name": "@ts-standard-library/string",
	"includeVersion": true,
	"exclude": ["**/*.test.ts"],
	"excludeExternals": true,
	"excludePrivate": true,
	"excludeProtected": true,
	"sort": ["source-order", "kind"]
}
