{
	"$schema": "https://typedoc.org/schema.json",
	"entryPointStrategy": "expand",
	"plugin": ["typedoc-plugin-markdown"],
	"theme": "markdown",
	"entryPoints": ["src/rng", "src/sorting"],
	"tsconfig": "./tsconfig.json",
	"out": "docs",
	"name": "@ts-standard-library/algorithms",
	"includeVersion": true,
	"exclude": ["**/*.test.ts"],
	"excludeExternals": true,
	"excludePrivate": true,
	"excludeProtected": true,
	"sort": ["source-order", "kind"]
}
